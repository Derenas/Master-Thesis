from lxml import etree

def xmltotxt():
    tree = etree.parse("data.xml")
    for user in tree.xpath("/users/user/nom"):
        print(user.text)

def read():
    readfile = open('../00a01.txt','r')
    for line in readfile.readlines():
        print line
    readfile.close()


if __name__ == '__main__':
    xmltotxt()
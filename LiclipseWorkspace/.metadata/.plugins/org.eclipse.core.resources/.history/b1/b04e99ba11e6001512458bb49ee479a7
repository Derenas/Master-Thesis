import os
from regularExpression import regularExpression

def getAllFiles():
    for dossier, sous_dossiers, fichiers in os.walk('./../../../../Avis_Decisions/pdftotext'):
#             print('##### %s #####' % dossier)
#             print("Sous dossiers : %s" % sous_dossier)
#             print("Fichiers : %s" % fichier)
        for fichier in fichiers:
            regularExpression.test(fichier)

if __name__ == '__main__':
    fichiers = getAllFiles()
    